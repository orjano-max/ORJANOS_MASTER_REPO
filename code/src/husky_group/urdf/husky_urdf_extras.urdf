<robot>
  <!-- Husky Urdf Extras -->
  <!-- 
  Addons to the clearpath husky is added in this file. 
  This file is passed when launching the husky and addons described here will be added to the husky
    -->

  <!-- Sensor frame link -->
  <link
    name="frame_link">
    <inertial>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <mass
        value="2.2936" />
      <inertia
        ixx="0.026381"
        ixy="1.2862E-08"
        ixz="-2.2196E-07"
        iyy="0.021942"
        iyz="2.0183E-08"
        izz="0.0063606" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="file://$(find husky_group)/meshes/frame_link.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.89804 0.91765 0.92941 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="file://$(find husky_group)/meshes/frame_link.STL" />
      </geometry>
    </collision>
  </link>

  <joint name="frame_joint" type="fixed">
    <parent link="user_rail_link"/>
    <child link="frame_link"/>
    <origin xyz="-0.01 0 0.01" rpy="0 0 0"/>
  </joint>
</robot>

 <!-- OUSTER LIDAR LINK. ØØ. 19.09.22 -->
  <link name="lidar_link">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.125"/>
      <inertia ixx="0.001"  ixy="0"  ixz="0" iyy="0.001" iyz="0" izz="0.001" />
    </inertial>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
         <cylinder radius="0.0508" length="0.055"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
         <cylinder radius="0.0508" length="0.055"/>
      </geometry>
    </visual>
  </link>

  <joint name="lidar_joint" type="fixed">
    <parent link="user_rail_link"/>
    <child link="lidar_link"/>
    <origin xyz="-0.0635 0 0.610" rpy="0 0 3.14"/>
  </joint>