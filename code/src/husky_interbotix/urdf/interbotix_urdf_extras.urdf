  <!-- %%%%%% CAMERA LINK %%%%% -->
  <link name="camera_link">
  </link>

  <joint name="camera_frame_joint" type="fixed">
    <parent link="$(arg robot_name)/gripper_link"/>
    <child link="camera_frame_link"/>
    <origin xyz="0.028 0 0.035" rpy="0 0 0"/>
  </joint>

